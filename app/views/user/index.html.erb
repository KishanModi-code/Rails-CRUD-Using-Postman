 %>
 <h1>Users</h1>
<% @users.each do |user| %>
  <div>
    <%= image_tag(user.avatar_url, width: '100', height: '100') if user.avatar_url.present? %><br>
    <strong>Name:</strong> <%= user.firstname %> <%= user.lastname %><br>
    <strong>Mobile Number:</strong> <%= user.mobilenumber %><br>
  </div>
<% end %>
